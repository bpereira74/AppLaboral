
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "devise/shared/error_messages", resource: resource %>

  <div class="field">
    <p align="center"><%= f.label :email %></p><br />
    <%= @user.email %>
  </div>

  <div class="field">
    <%= f.label :password %>

  <div class="field">
    <%= f.label :"Tu foto" %><br />
    <%= f.file_field :image %>
  </div>
